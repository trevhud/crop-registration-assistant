{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "/_next/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/api/server",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}